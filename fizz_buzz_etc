#include <iostream>

int main() {

  //for loop to print numbers 1 to 100
  for (int i = 1; i <= 100; i++) {
    
    //check if i is divisible by 3 and if so print "fizz"
    if (i % 3 == 0) {
      std::cout << "Fizz!\n";
    }

    //check if i is divisible by 5 and if so print "buzz"
    else if (i % 5 == 0) {
      std::cout << "Buzz!\n";
    }

    //check if i is divisible by 7 and if so print "woof"
    else if (i % 7 == 0){
      std::cout << "Woof!\n";
    }
    
    //check if i is the number 13 and if so print "boo"
    else if (i == 13) {
      std::cout << "BOO!\n";
    }

    //check if i is divisible by 3 and 5 (15) and if so print "fizzbuzz"
    else if (i % 15 == 0) {
      std::cout << "Fizz Buzz!\n";
    }

    //if none of the 3 conditions apply, print the number (i)
    else {
      std::cout << i << "\n";
    }
    
  }

}

  
